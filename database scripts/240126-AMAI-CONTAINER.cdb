CREATE TABLE AMA_CONTAINER_TAB (
    Container_Id NUMBER DEFAULT NULL,
    Current_Weight NUMBER,
    Max_Volume FLOAT,
    Max_Weight FLOAT,
    Current_Volume FLOAT,
    Branch_Id NUMBER,
    Company_Reg_No NUMBER,
    PRIMARY KEY (Container_Id,Company_Reg_No,Branch_Id)
);

CREATE SEQUENCE CONTAINER_SEQ
	START WITH 1
	INCREMENT BY 1
	NOCACHE
	NOCYCLE;